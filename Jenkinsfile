pipeline {
    agent any
    stages {
        stage ('Compile') {
            steps {
                script {
                    echo "Compile the code"
                }
            }
        }
        stage ('Unittest') {
            steps {
                script {
                    echo "Test the code"
                }
            }
        }
        stage ('Package') {
            steps {
                script {
                    echo "Package the code"
                }
            }
        }
    }
}